  "main": "electron/electron.js",
  "build": {
    "appId": "com.iuth.accountmanager",
    "productName": "IUTH Account Manager",
    "asar": false,
    "files": [
      "public/**/*",
      "electron/*"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "src/assets/img/IUTHlogo.ico"
    }
  },
