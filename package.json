  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "build": "ng build",
    "test": "ng test",
    "electron dist": "electron .",
    "electron": "ng build && electron .",
    "electron-aot": "ng build --prod && electron .",
    "lint": "ng lint",
    "dist": "build",
    "e2e": "ng e2e",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && ng serve -o",
